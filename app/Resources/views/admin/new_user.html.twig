{% extends '::base_admin.html.twig' %}
{% block title %}Content manager page || Users{% endblock %}

{% block content %}
    <h2 class="content_title">New administrator account</h2>
    <div class="user">
        {{ include('flash-messages.html.twig') }}

        {{ form_start(form, {'name': 'new_user', 'method': 'post', 'attr': { 'autocomplete': 'off' } }) }}

        {{ form_row(form.username) }}
        {{ form_row(form.password) }}

        {{ form_end(form) }}
    </div>

{% endblock %}

